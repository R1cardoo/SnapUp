<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.snapup_android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />

    <application
        android:name="com.example.snapup_android.MyApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.SnapUp_Android" >
        <activity android:name="com.example.snapup_android.login.RegisterActivity" />
        <activity
            android:name="com.example.snapup_android.Homepage.HomepageActivity"
            android:label="@string/title_activity_homepage"
            android:theme="@style/Theme.SnapUp_Android.NoActionBar" />
        <activity android:name="com.example.snapup_android.viewSchedule.TicketActivity" />
        <activity android:name="com.example.snapup_android.settings.SettingsActivity" />
        <activity android:name="com.example.snapup_android.viewSchedule.ScheduleInfo" />
        <activity android:name="com.example.snapup_android.viewOrder.OrderInfo" />
        <activity
            android:name="com.example.snapup_android.feedback.FeedbackActivity"
            android:label="@string/title_activity_feedback"
            android:theme="@style/Theme.SnapUp_Android.NoActionBar" />
        <activity android:name="com.example.snapup_android.completeOrder.CompleteOrderActivity" />
        <activity
            android:name="com.example.snapup_android.viewSchedule.ViewScheduleActivity"
            android:label="@string/title_activity_view_info"
            android:theme="@style/Theme.SnapUp_Android.NoActionBar" />
        <activity
            android:name="com.example.snapup_android.viewOrder.ViewOrderActivity"
            android:label="@string/title_activity_view_order"
            android:theme="@style/Theme.SnapUp_Android.NoActionBar" />
        <activity android:name="com.example.snapup_android.login.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>