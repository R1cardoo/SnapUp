<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.snapup_android.Homepage.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/nav_info"
        android:name="com.example.snapup_android.viewInfo.ScheduleList"
        android:label="班次信息"
        tools:layout="@layout/fragment_schedule_list" />
    <fragment
        android:id="@+id/nav_order"
        android:name="com.example.snapup_android.viewOrder.FirstFragment"
        android:label="我的订单"
        tools:layout="@layout/activity_view_order" />
    <fragment
        android:id="@+id/nav_feedback"
        android:name="com.example.snapup_android.feedback.First3Fragment"
        android:label="反馈"
        tools:layout="@layout/activity_feedback" />
    <fragment
        android:id="@+id/nav_quit"
        android:name="com.example.snapup_android.login.LoginActivity"
