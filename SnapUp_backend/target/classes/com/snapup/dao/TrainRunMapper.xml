<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.snapup.dao.TrainRunMapper">
    <select id="findTrainRunByCode" resultType="TrainRun">
        select * from snapup.train_run
        where run_code = #{run_code}
    </select>
    <select id="findAllTrainRun" resultType="TrainRun">
        select * from snapup.train_run
    </select>
    <delete id="delLine">
        select * from snapup.train_run
        where run_code = #{run_code}
    </delete>

</mapper>